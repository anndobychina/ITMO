<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>HomeWork</servlet-name>
		<servlet-class>homework.HomeWorkServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeWork</servlet-name>
		<url-pattern>/homework</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>	
	<servlet>
		<servlet-name>Java2Servlet</servlet-name>
		<servlet-class>homework.Java2Servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Java3Servlet</servlet-name>
		<servlet-class>homework.Java3Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Java2Servlet</servlet-name>
		<url-pattern>/java_2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Java3Servlet</servlet-name>
		<url-pattern>/java_3</url-pattern>
	</servlet-mapping>
</web-app>
